@model MVC_Team_Git_JooleMay.Models.VMProductList

    @{
        ViewBag.Title = "ProductList";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

 
@foreach (var p in Model.)
{
    <div class="item">
        <h3>@p.Name</h3>
        @p.Description
        <h4>@p.Price.ToString("c")</h4>
    </div>
}